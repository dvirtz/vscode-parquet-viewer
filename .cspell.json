{
    "version": "0.2",
    "language": "en",
    "words": [
        "commitlint",
        "devcontainer",
        "dvirtz",
        "kbajalc",
        "prebuilds",
        "tempy",
        "Yitzchaki"
    ],
    "ignorePaths": [
        "**/node_modules/**",
        "**/build/**",
        "**/dist/**"
    ],
    "flagWords": [],
    "languageSettings": [
        {
            "languageId": "markdown",
            "ignoreRegExpList": [
                "/`.*?`/", // inline code
                "/(```+)\\s?[\\s\\S]+?\\1/g", // code block
                "/'s\\b/", // trailing 's
                "<!--.*?-->" // comments
            ]
        },
        {
            "languageId": "git",
            "ignoreRegExpList": [
                "/#\\s.*/", // comments
                "/`.*?`/" // inline code
            ]
        }
    ]
}
