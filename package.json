{
  "name": "parquet-viewer",
  "displayName": "parquet-viewer",
  "description": "Views Apache Parquet files as JSON",
  "publisher": "dvirtz",
  "icon": "images/ico128.png",
  "homepage": "https://github.com/dvirtz/vscode-parquet-viewer",
	"bugs": {
		"url": "https://github.com/dvirtz/vscode-parquet-viewer/issues",
		"email": "smcbreen@microsoft.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dvirtz/vscode-parquet-viewer.git"
  },
  "author": {
    "name": "Dvir Yitzchaki",
    "email": "dvirt@gmail.com",
    "url": "https://github.com/dvirtz"
  },
  "license": "SEE LICENSE AT LICENSE.txt",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "parquet",
    "spark",
    "viewer"
  ],
  "activationEvents": [
    "onLanguage:parquet"
  ],
  "main": "./out/extension",
  "contributes": {
    "languages": [
      {
        "id": "parquet",
        "extensions": [
          ".parquet"
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.viewParquetAsJson",
        "category": "Parquet",
        "title": "View as JSON"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}