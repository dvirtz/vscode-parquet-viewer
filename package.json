{
  "name": "parquet-viewer",
  "displayName": "parquet-viewer",
  "description": "Views Apache Parquet files as JSON",
  "publisher": "dvirtz",
  "icon": "images/ico128.png",
  "homepage": "https://github.com/dvirtz/vscode-parquet-viewer",
  "bugs": {
    "url": "https://github.com/dvirtz/vscode-parquet-viewer/issues",
    "email": "smcbreen@microsoft.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dvirtz/vscode-parquet-viewer.git"
  },
  "author": {
    "name": "Dvir Yitzchaki",
    "email": "dvirt@gmail.com",
    "url": "https://github.com/dvirtz"
  },
  "license": "MIT",
  "version": "2.2.2",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "parquet",
    "spark",
    "viewer"
  ],
  "activationEvents": [
    "onCustomEditor:parquetViewer.parquetViewer"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "configuration": [
      {
        "title": "parquet-viewer",
        "properties": {
          "parquet-viewer.parquetToolsPath": {
            "type": "string",
            "default": "parquet-tools",
            "description": "Path to parquet-tools executable or jar"
          },
          "parquet-viewer.logPanel": {
            "description": "Write diagnostic logs to an output panel",
            "type": "boolean",
            "markdownDeprecationMessage": "**Deprecated**: Please use `#parquet-viewer.logging.panel#` instead."
          },
          "parquet-viewer.logFolder": {
            "description": "Write diagnostic logs under the given directory",
            "type": "string",
            "markdownDeprecationMessage": "**Deprecated**: Please use `#parquet-viewer.logging.folder#` instead."
          },
          "parquet-viewer.logLevel": {
            "description": "Log level",
            "type": "string",
            "enum": [
              "off",
              "fatal",
              "error",
              "warn",
              "info",
              "debug",
              "trace"
            ],
            "default": "info",
            "markdownDeprecationMessage": "**Deprecated**: Please use `#parquet-viewer.logging.level#` instead."
          },
          "parquet-viewer.useParquetTools": {
            "description": "Use legacy parquet-tools application for reading parquet files",
            "type": "boolean",
            "markdownDeprecationMessage": "Use `#parquet-viewer.backend#` configuration instead"
          },
          "parquet-viewer.logging.panel": {
            "description": "Write diagnostic logs to an output panel",
            "type": "boolean"
          },
          "parquet-viewer.logging.folder": {
            "description": "Write diagnostic logs under the given directory",
            "type": "string"
          },
          "parquet-viewer.logging.level": {
            "description": "Log level",
            "type": "string",
            "enum": [
              "off",
              "fatal",
              "error",
              "warn",
              "info",
              "debug",
              "trace"
            ],
            "default": "info"
          },
          "parquet-viewer.jsonSpace": {
            "markdownDescription": "JSON indentation space, passed to `JSON.stringify` as is, see [mdn](https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/JSON/stringify#parameters) for details. Only applies when `#parquet-viewer.useParquetTools#` is `false`.",
            "type": [
              "string",
              "number"
            ],
            "default": 0,
            "minimum": 0,
            "maximum": 10,
            "maxLength": 10
          },
          "parquet-viewer.backend": {
            "markdownDescription": "Which backend to use for reading the files",
            "type": "string",
            "enum": [
              "parquet-tools",
              "parquets",
              "arrow"
            ],
            "default": "arrow"
          }
        }
      }
    ],
    "customEditors": [
      {
        "viewType": "parquetViewer.parquetViewer",
        "displayName": "Parquet Viewer",
        "selector": [
          {
            "filenamePattern": "*.parquet"
          }
        ]
      }
    ]
  },
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": "limited",
      "restrictedConfigurations": [
        "parquet-viewer.useParquetTools",
        "parquet-viewer.parquetToolsPath"
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p tsconfig.json",
    "watch": "tsc -watch -p .",
    "preintegration": "npm run compile",
    "integration": "node ./out/test/integration/runTests.js",
    "unit": "jest --verbose test/unit",
    "test": "npm run unit && npm run integration",
    "spellcheck": "cspell **/*.ts **/*.md",
    "lint": "eslint --max-warnings=0 **/*.ts",
    "prepare": "husky install",
    "addon:build": "cmake-js build -d src/arrow-parquet-reader && copyfiles src/arrow-parquet-reader/build/Release/arrow-parquet-reader.node out",
    "addon:build:debug": "cmake-js build -d src/arrow-parquet-reader -D && copyfiles src/arrow-parquet-reader/build/Debug/arrow-parquet-reader.node out"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/bindings": "^1.5.1",
    "@types/glob": "^7.1.3",
    "@types/jest": "^28.1.1",
    "@types/node": "^12.20.11",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vscode/test-electron": "^2.1.3",
    "cmake-js": "^7.1.1",
    "copyfiles": "^2.4.1",
    "cspell": "^5.11.0",
    "eslint": "^8.17.0",
    "glob-promise": "^4.2.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "semantic-release": "^19.0.3",
    "semantic-release-vsce": "^5.0.17",
    "tempy": "^1.0.1",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@async-generators/to-array": "^0.1.0",
    "@dvirtz/parquets": "^0.11.6",
    "@vscode-logging/logger": "^1.2.3",
    "@vscode-logging/wrapper": "^1.0.1",
    "apache-arrow": "^9.0.0",
    "bindings": "^1.5.0",
    "color-string": "^1.6.0",
    "memory-streams": "^0.1.3",
    "node-addon-api": "^5.0.0"
  },
  "__metadata": {
    "id": "b0d61cc0-c99f-47d2-88eb-6b26dc2a5b1e",
    "publisherDisplayName": "Dvir Yitzchaki",
    "publisherId": "5c0bfd53-c85a-4ae0-9171-da96c2ed64f3",
    "isPreReleaseVersion": false
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        2,
        "always",
        200
      ],
      "footer-max-line-length": [
        0,
        "always"
      ]
    }
  }
}
